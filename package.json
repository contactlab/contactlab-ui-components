{
  "name": "contactlab-ui-components",
  "version": "1.4.17",
  "description": "Polymer UI components of the Contactlab Pattern Library",
  "main": "index.js",
  "engines": {
    "node": ">= 8.0",
    "yarn": ">= 1.1"
  },
  "scripts": {
    "sass": "node ./scripts/sass",
    "start": "http-server -s -p 3000",
    "dev": "concurrently -k \"http-server -s -p 3000\" \"webpack -w \" ",
    "test": "ava src/**/test/**.js --verbose"
  },
  "files": [
    "src",
    "assets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contactlab/contactlab-ui-components"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "author": "ContactLab Product Team",
  "license": "Apache-2.0",
  "homepage": "https://github.com/contactlab/contactlab-ui-components#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "babel-core": "6.26.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "concurrently": "3.6.0",
    "http-server": "0.11.1",
    "jsdom-wc": "11.0.0-alpha-1",
    "node-sass": "4.9.0",
    "strip-loader": "0.1.2",
    "wc-loader": "1.1.12",
    "webpack": "3.10.0",
    "webpack-notifier": "1.6.0"
  },
  "dependencies": {
    "escape-string-regexp": "1.0.5",
    "ikonograph": "2.0.0",
    "moment": "2.20.1",
    "rome": "2.1.22",
    "web-animations-js": "2.3.1",
    "webcomponents.js": "0.7.24"
  }
}
