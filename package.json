{
  "name": "contactlab-ui-components",
  "version": "2.0.7",
  "description": "Polymer (v3) UI components of the Contactlab Pattern Library",
  "main": "index.js",
  "files": [
    "src",
    "assets"
  ],
  "config": {
    "port": {
      "dev": 3000
    },
    "buildType": null
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "start": "http-server -s -p 3000",
    "dev": "concurrently -k \"http-server -s -p 3000\" \"webpack -w \" ",
    "sass": "node-sass ./assets/scss/contactlab-pattern.scss > ./assets/css/contactlab-pattern.css",
    "postsass": "node-sass --output-style compressed ./assets/scss/contactlab-pattern.scss > ./assets/css/contactlab-pattern.min.css",
    "build": "webpack",
    "build:test": "webpack --config webpack.config-test.js",
    "pretest": "webpack",
    "test": "ava ./test/index.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contactlab/contactlab-ui-components"
  },
  "author": "ContactLab Product Team",
  "license": "Apache-2.0",
  "homepage": "https://github.com/contactlab/contactlab-ui-components#readme",
  "devDependencies": {
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "concurrently": "3.5.0",
    "flow-bin": "0.53.1",
    "http-server": "0.10.0",
    "jsdom-wc": "11.0.0-alpha-1",
    "node-sass": "4.5.3",
    "text-loader": "0.0.1",
    "webpack": "3.5.5",
    "webpack-notifier": "1.5.0"
  },
  "dependencies": {
    "@polymer/polymer": "next",
    "@webcomponents/webcomponentsjs": "1.0.8",
    "ikonograph": "2.0.0",
    "moment": "2.18.1",
    "rome": "2.1.22",
    "web-animations-js": "2.3.1"
  }
}
