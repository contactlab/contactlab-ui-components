  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "author": "ContactLab Product Team",
  "license": "Apache-2.0",
  "homepage": "https://github.com/contactlab/contactlab-ui-components#readme",
  "devDependencies": {
    "ava": "0.22.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "concurrently": "3.4.0",
    "http-server": "0.10.0",
    "jsdom-wc": "11.0.0-alpha-1",
    "node-sass": "4.5.0",
    "strip-loader": "0.1.2",
    "wc-loader": "1.1.4",
    "webpack": "3.0.0",
    "webpack-notifier": "1.5.0"
  },
  "dependencies": {
    "ikonograph": "2.0.0",
    "moment": "2.17.1",
    "rome": "2.1.22",
    "web-animations-js": "2.2.5",
    "webcomponents.js": "0.7.24"
